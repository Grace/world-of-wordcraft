{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug TypeScript",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/web/src/script.ts",
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "sourceMaps": true,
        "outFiles": [
          "${workspaceFolder}/web/dist/**/*.js"
        ],
        "console": "integratedTerminal"
      }
    ]
  }